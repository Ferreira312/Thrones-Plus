<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
  <!--===========================================Throne===========================================-->
 
<ThingDef ParentName="FurnitureWithQualityBase">
    <defName>ThroneA</defName>
    <label>Raid Stabber throne</label>
    <drawGUIOverlay>true</drawGUIOverlay>
    <drawGUIOverlayQuality>false</drawGUIOverlayQuality>
    <description>A chair designed for a dignity-focused psycaster to sit and meditate. Reigning on a throne is a form of meditation that builds up psyfocus. For dignity psycasters, more impressive meditation thrones and thronerooms increase the rate of psyfocus gain.</description>
    <defaultPlacingRot>South</defaultPlacingRot>
    <altitudeLayer>Building</altitudeLayer>
    <category>Building</category>
    <thingSetMakerTags><li>RewardStandardQualitySuper</li></thingSetMakerTags>
    <hasInteractionCell>true</hasInteractionCell>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>10000</WorkToBuild>
      <Mass>40</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>4</Beauty>
      <Comfort>0.75</Comfort>
      <MeditationFocusStrength>0.15</MeditationFocusStrength>
    </statBases>
    <thingClass>Building_Throne</thingClass>
    <socialPropernessMatters>true</socialPropernessMatters>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>125</costStuffCount>
    <pathCost>30</pathCost>
    <fillPercent>0.4</fillPercent>
    <building>
      <isSittable>true</isSittable>
    </building>
    <uiIconOffset>(0, 0.08)</uiIconOffset>
    <uiIconScale>0.75</uiIconScale>
    <comps>
      <li Class="CompProperties_AssignableToPawn">
        <drawAssignmentOverlay>true</drawAssignmentOverlay>
        <compClass>CompAssignableToPawn_Throne</compClass>
      </li>
      <li Class="CompProperties_MeditationFocus">
        <statDef>MeditationFocusStrength</statDef>
        <focusTypes>
          <li>Dignified</li>
        </focusTypes>
        <offsets>
          <li Class="FocusStrengthOffset_ThroneSatisfiesRequirements">
            <offset>0.08</offset>
          </li>
          <li Class="FocusStrengthOffset_RoomImpressiveness">
            <curve>
              <points>
                <li>(0, -0.05)</li>
                <li>(30, 0.0)</li>
                <li>(60, 0.04)</li>
                <li>(100,  0.08)</li>
                <li>(170,  0.1)</li>
              </points>
            </curve>
          </li>
        </offsets>
      </li>
    </comps>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
    <graphicData>
      <drawSize>(2.4,2.4)</drawSize>
      <texPath>Building/Furniture/Throne_A</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shadowData>
        <volume>(0.5,0.35,0.4)</volume>
      </shadowData>
      <damageData>
        <rect>(0.2,0,2, 0.6, 0.6)</rect>
      </damageData>
    </graphicData>
  </ThingDef>
 
 
